{
  "name": "Platform Observability \u2014 Bronze Job + DLT Silver/Gold",
  "schedule": {
    "quartz_cron_expression": "0 30 5 ? * *",
    "timezone_id": "Asia/Kolkata",
    "pause_status": "UNPAUSED"
  },
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "bronze_hwm_job",
      "notebook_task": {
        "notebook_path": "/Repos/<your-repo-path>/notebooks/bronze_hwm_ingest_job.py",
        "base_parameters": {
          "overlap_hours": "48"
        }
      },
      "existing_cluster_id": "<CLUSTER_ID>"
    },
    {
      "task_key": "silver_gold_dlt",
      "depends_on": [
        {
          "task_key": "bronze_hwm_job"
        }
      ],
      "pipeline_task": {
        "pipeline_id": "<SILVER_GOLD_PIPELINE_ID>",
        "full_refresh": false
      }
    }
  ]
}