{
  "name": "simple_dlt_pipeline",
  "description": "Simple DLT pipeline for generating billing usage test data",
  "libraries": [
    {
      "notebook": {
        "path": "/notebooks/sample_dlt_pipeline"
      }
    }
  ],
  "target": "platform_observability.plt_bronze",
  "catalog": "platform_observability",
  "configuration": {
    "spark.databricks.delta.retentionDurationCheck.enabled": "false"
  },
  "clusters": [
    {
      "label": "default",
      "node_type_id": "i3.xlarge",
      "driver_node_type_id": "i3.xlarge",
      "num_workers": 0
    }
  ],
  "development": true,
  "continuous": false,
  "photon": false,
  "edition": "CORE",
  "channel": "CURRENT",
  "storage_location": "s3://your-bucket/platform-observability/simple-dlt-pipeline"
}
