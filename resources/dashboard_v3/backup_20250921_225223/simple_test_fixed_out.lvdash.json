            }
          }
        },
        {
          "displayName": "End Date",
          "keyword": "param_end_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "2025-09-21T00:00:00.000"
                }
              ]
            }
          }
        }
      ]
    }
