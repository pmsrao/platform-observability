    "cost_anomalies": {
      "name": "dataset_007",
      "displayName": "Cost Anomalies with Context",
      "queryLines": [
        "SELECT ",
        "    TO_DATE(CAST(date_key AS STRING), 'yyyyMMdd') as date,",
        "    workspace_id,",
        "    billing_origin_product as workload_type,",
        "    job_pipeline_id,",
        "    ROUND(usage_cost, 2) as cost,",
        "    anomaly_flag,",
        "    CASE ",
        "        WHEN anomaly_flag = 'HIGH_COST_ANOMALY' THEN 'Cost Spike Detected' ",
        "        WHEN anomaly_flag = 'LOW_COST_ANOMALY' THEN 'Unusual Low Cost' ",
        "        ELSE 'Normal' ",
        "    END as anomaly_description ",
        "FROM {catalog}.{gold_schema}.v_cost_anomalies ",
        "WHERE date_key >= date_format(:param_start_date, 'yyyyMMdd') ",
        "  AND date_key <= date_format(:param_end_date, 'yyyyMMdd') ",
        "  AND IF(:param_workspace = '<ALL_WORKSPACES>', true, workspace_id = :param_workspace) ",
        "  AND anomaly_flag != 'NORMAL' ",
        "ORDER BY usage_cost DESC "
      ],
      "parameters": [
        {
          "displayName": "Start Date",
          "keyword": "param_start_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [{"value": "2024-08-20T00:00:00.000"}]
            }
          }
        },
        {
          "displayName": "End Date",
          "keyword": "param_end_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
